{
  "name": "vscode-unify-chat-provider",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "4.7.0",
  "categories": [
    "AI",
    "Chat"
  ],
  "keywords": [
    "ai",
    "chat",
    "agent",
    "copilot",
    "github-copilot",
    "language-model",
    "llm",
    "ucp",
    "oai",
    "openai",
    "anthropic",
    "gemini",
    "ollama",
    "deepseek",
    "qwen",
    "minimax",
    "moonshot",
    "kimi",
    "glm",
    "zhipu",
    "zai",
    "hugging-face",
    "openrouter",
    "model-studio",
    "modelscope",
    "volcano-engine",
    "byteplus",
    "hunyuan",
    "hy",
    "xiaomimimo",
    "iflow",
    "claude-code",
    "codex",
    "gemini-cli",
    "antigravity",
    "qwen-code",
    "kiro",
    "free-ai",
    "free-chat",
    "free-agent",
    "free-models",
    "coding-plan",
    "custom-models",
    "custom-chat",
    "自定义模型",
    "自定义聊天",
    "免费模型",
    "免费聊天"
  ],
  "publisher": "SmallMain",
  "icon": "icon.png",
  "repository": "github:smallmain/vscode-unify-chat-provider",
  "bugs": "https://github.com/smallmain/vscode-unify-chat-provider/issues",
  "author": "SmallMain",
  "sponsor": {
    "url": "https://github.com/smallmain/sponsors"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.104.0"
  },
  "packageManager": "npm@10.8.2",
  "extensionDependencies": [
    "GitHub.copilot-chat"
  ],
  "enabledApiProposals": [
    "extensionsAny",
    "newSymbolNamesProvider",
    "interactive",
    "codeActionAI",
    "activeComment",
    "commentReveal",
    "contribCommentThreadAdditionalMenu",
    "contribCommentsViewThreadMenus",
    "documentFiltersExclusive",
    "embeddings",
    "findTextInFiles",
    "findTextInFiles2",
    "findFiles2",
    "textSearchProvider",
    "terminalDataWriteEvent",
    "terminalExecuteCommandEvent",
    "terminalSelection",
    "terminalQuickFixProvider",
    "mappedEditsProvider",
    "aiRelatedInformation",
    "aiSettingsSearch",
    "chatParticipantAdditions",
    "chatEditing",
    "defaultChatParticipant",
    "contribSourceControlInputBoxMenu",
    "authLearnMore",
    "testObserver",
    "aiTextSearchProvider",
    "chatParticipantPrivate",
    "chatProvider",
    "contribDebugCreateConfiguration",
    "chatReferenceDiagnostic",
    "textSearchProvider2",
    "chatReferenceBinaryData",
    "languageModelSystem",
    "languageModelCapabilities",
    "inlineCompletionsAdditions",
    "chatStatusItem",
    "taskProblemMatcherStatus",
    "contribLanguageModelToolSets",
    "textDocumentChangeReason",
    "resolvers",
    "taskExecutionTerminal",
    "dataChannels",
    "languageModelThinkingPart",
    "chatSessionsProvider",
    "devDeviceId",
    "contribEditorContentMenu"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "languageModelChatProviders": [
      {
        "vendor": "unify-chat-provider",
        "displayName": "%languageModelChatProviders.displayName%",
        "managementCommand": "unifyChatProvider.manageProviders"
      }
    ],
    "commands": [
      {
        "command": "unifyChatProvider.addProvider",
        "title": "%command.addProvider.title%"
      },
      {
        "command": "unifyChatProvider.removeProvider",
        "title": "%command.removeProvider.title%"
      },
      {
        "command": "unifyChatProvider.importConfig",
        "title": "%command.importConfig.title%"
      },
      {
        "command": "unifyChatProvider.addProviderFromWellKnownProviderList",
        "title": "%command.addProviderFromWellKnownProviderList.title%"
      },
      {
        "command": "unifyChatProvider.importConfigFromOtherApplications",
        "title": "%command.importConfigFromOtherApplications.title%"
      },
      {
        "command": "unifyChatProvider.exportConfig",
        "title": "%command.exportConfig.title%"
      },
      {
        "command": "unifyChatProvider.manageProviders",
        "title": "%command.manageProviders.title%"
      },
      {
        "command": "unifyChatProvider.manageBalances",
        "title": "%command.manageBalances.title%"
      },
      {
        "command": "unifyChatProvider.refreshAllProvidersOfficialModels",
        "title": "%command.refreshAllProvidersOfficialModels.title%"
      },
      {
        "command": "unifyChatProvider.refreshAllProvidersBalance",
        "title": "%command.refreshAllProvidersBalance.title%"
      }
    ],
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "unifyChatProvider.verbose": {
          "type": "boolean",
          "description": "%configuration.verbose.description%",
          "default": false
        },
        "unifyChatProvider.storeApiKeyInSettings": {
          "type": "boolean",
          "description": "%configuration.storeApiKeyInSettings.description%",
          "default": false
        },
        "unifyChatProvider.balanceRefreshIntervalMs": {
          "type": "integer",
          "description": "%configuration.balanceRefreshIntervalMs.description%",
          "default": 60000,
          "minimum": 1000
        },
        "unifyChatProvider.balanceThrottleWindowMs": {
          "type": "integer",
          "description": "%configuration.balanceThrottleWindowMs.description%",
          "default": 10000,
          "minimum": 0
        },
        "unifyChatProvider.balanceStatusBarIcon": {
          "type": "string",
          "description": "%configuration.balanceStatusBarIcon.description%",
          "default": "$(credit-card)"
        },
        "unifyChatProvider.balanceWarning.enabled": {
          "type": "boolean",
          "description": "%configuration.balanceWarning.enabled.description%",
          "default": true,
          "order": 1
        },
        "unifyChatProvider.balanceWarning.timeThresholdDays": {
          "type": "number",
          "description": "%configuration.balanceWarning.timeThresholdDays.description%",
          "default": 1,
          "minimum": 0,
          "order": 2
        },
        "unifyChatProvider.balanceWarning.amountThreshold": {
          "type": "number",
          "description": "%configuration.balanceWarning.amountThreshold.description%",
          "default": 1,
          "minimum": 0,
          "order": 3
        },
        "unifyChatProvider.balanceWarning.tokenThresholdMillions": {
          "type": "number",
          "description": "%configuration.balanceWarning.tokenThresholdMillions.description%",
          "default": 1,
          "minimum": 0,
          "order": 4
        },
        "unifyChatProvider.networkSettings": {
          "type": "object",
          "description": "%configuration.networkSettings.description%",
          "properties": {
            "timeout": {
              "type": "object",
              "description": "%configuration.networkSettings.timeout.description%",
              "properties": {
                "connection": {
                  "type": "integer",
                  "description": "%configuration.networkSettings.timeout.connection.description%",
                  "default": 60000
                },
                "response": {
                  "type": "integer",
                  "description": "%configuration.networkSettings.timeout.response.description%",
                  "default": 300000
                }
              }
            },
            "retry": {
              "type": "object",
              "description": "%configuration.networkSettings.retry.description%",
              "properties": {
                "maxRetries": {
                  "type": "integer",
                  "description": "%configuration.networkSettings.retry.maxRetries.description%",
                  "default": 10
                },
                "initialDelayMs": {
                  "type": "integer",
                  "description": "%configuration.networkSettings.retry.initialDelayMs.description%",
                  "default": 1000
                },
                "maxDelayMs": {
                  "type": "integer",
                  "description": "%configuration.networkSettings.retry.maxDelayMs.description%",
                  "default": 60000
                },
                "backoffMultiplier": {
                  "type": "number",
                  "description": "%configuration.networkSettings.retry.backoffMultiplier.description%",
                  "default": 2
                },
                "jitterFactor": {
                  "type": "number",
                  "description": "%configuration.networkSettings.retry.jitterFactor.description%",
                  "default": 0.1
                }
              }
            }
          }
        },
        "unifyChatProvider.endpoints": {
          "type": "array",
          "description": "%configuration.endpoints.description%",
          "items": {
            "type": "object",
            "required": [
              "type",
              "name",
              "baseUrl",
              "models"
            ],
            "properties": {
              "type": {
                "type": "string",
                "description": "%configuration.endpoints.type.description%",
                "enum": [
                  "anthropic",
                  "google-ai-studio",
                  "google-vertex-ai",
                  "google-antigravity",
                  "google-gemini-cli",
                  "github-copilot",
                  "openai-chat-completion",
                  "qwen-code",
                  "openai-responses",
                  "openai-codex",
                  "ollama",
                  "claude-code"
                ],
                "enumDescriptions": [
                  "%configuration.endpoints.type.enumDescriptions.0%",
                  "%configuration.endpoints.type.enumDescriptions.1%",
                  "%configuration.endpoints.type.enumDescriptions.2%",
                  "%configuration.endpoints.type.enumDescriptions.6%",
                  "%configuration.endpoints.type.enumDescriptions.8%",
                  "%configuration.endpoints.type.enumDescriptions.10%",
                  "%configuration.endpoints.type.enumDescriptions.3%",
                  "%configuration.endpoints.type.enumDescriptions.11%",
                  "%configuration.endpoints.type.enumDescriptions.4%",
                  "%configuration.endpoints.type.enumDescriptions.7%",
                  "%configuration.endpoints.type.enumDescriptions.5%",
                  "%configuration.endpoints.type.enumDescriptions.9%"
                ]
              },
              "name": {
                "type": "string",
                "description": "%configuration.endpoints.name.description%"
              },
              "baseUrl": {
                "type": "string",
                "description": "%configuration.endpoints.baseUrl.description%"
              },
              "balanceProvider": {
                "type": "object",
                "description": "%configuration.endpoints.balanceProvider.description%",
                "oneOf": [
                  {
                    "properties": {
                      "method": {
                        "type": "string",
                        "const": "none",
                        "description": "%configuration.endpoints.balanceProvider.method.none.description%"
                      }
                    },
                    "required": [
                      "method"
                    ]
                  },
                  {
                    "properties": {
                      "method": {
                        "type": "string",
                        "const": "moonshot-ai",
                        "description": "%configuration.endpoints.balanceProvider.method.moonshot.description%"
                      }
                    },
                    "required": [
                      "method"
                    ]
                  },
                  {
                    "properties": {
                      "method": {
                        "type": "string",
                        "const": "kimi-code",
                        "description": "%configuration.endpoints.balanceProvider.method.kimiCode.description%"
                      }
                    },
                    "required": [
                      "method"
                    ]
                  },
                  {
                    "properties": {
                      "method": {
                        "type": "string",
                        "const": "newapi",
                        "description": "%configuration.endpoints.balanceProvider.method.newapi.description%"
                      },
                      "userId": {
                        "type": "string",
                        "description": "%configuration.endpoints.balanceProvider.newapi.userId.description%"
                      },
                      "systemToken": {
                        "type": "string",
                        "description": "%configuration.endpoints.balanceProvider.newapi.systemToken.description%"
                      }
                    },
                    "required": [
                      "method"
                    ]
                  }
                ]
              },
              "contextCache": {
                "type": "object",
                "description": "%configuration.endpoints.contextCache.description%",
                "properties": {
                  "type": {
                    "type": "string",
                    "description": "%configuration.endpoints.contextCache.type.description%",
                    "enum": ["only-free", "allow-paid"],
                    "default": "only-free",
                    "enumDescriptions": [
                      "%configuration.endpoints.contextCache.type.enumDescriptions.0%",
                      "%configuration.endpoints.contextCache.type.enumDescriptions.1%"
                    ]
                  },
                  "ttl": {
                    "type": "integer",
                    "description": "%configuration.endpoints.contextCache.ttl.description%",
                    "minimum": 1
                  }
                }
              },
              "extraHeaders": {
                "type": "object",
                "description": "%configuration.endpoints.extraHeaders.description%",
                "additionalProperties": {
                  "type": "string"
                }
              },
              "extraBody": {
                "type": "object",
                "description": "%configuration.endpoints.extraBody.description%",
                "additionalProperties": true
              },
              "timeout": {
                "type": "object",
                "description": "%configuration.endpoints.timeout.description%",
                "properties": {
                  "connection": {
                    "type": "integer",
                    "description": "%configuration.endpoints.timeout.connection.description%",
                    "default": 60000
                  },
                  "response": {
                    "type": "integer",
                    "description": "%configuration.endpoints.timeout.response.description%",
                    "default": 300000
                  }
                }
              },
              "retry": {
                "type": "object",
                "description": "%configuration.endpoints.retry.description%",
                "properties": {
                  "maxRetries": {
                    "type": "integer",
                    "description": "%configuration.endpoints.retry.maxRetries.description%",
                    "default": 10
                  },
                  "initialDelayMs": {
                    "type": "integer",
                    "description": "%configuration.endpoints.retry.initialDelayMs.description%",
                    "default": 1000
                  },
                  "maxDelayMs": {
                    "type": "integer",
                    "description": "%configuration.endpoints.retry.maxDelayMs.description%",
                    "default": 60000
                  },
                  "backoffMultiplier": {
                    "type": "number",
                    "description": "%configuration.endpoints.retry.backoffMultiplier.description%",
                    "default": 2
                  },
                  "jitterFactor": {
                    "type": "number",
                    "description": "%configuration.endpoints.retry.jitterFactor.description%",
                    "default": 0.1
                  }
                }
              },
              "autoFetchOfficialModels": {
                "type": "boolean",
                "description": "%configuration.endpoints.autoFetchOfficialModels.description%",
                "default": false
              },
              "models": {
                "type": "array",
                "description": "%configuration.endpoints.models.description%",
                "items": {
                  "oneOf": [
                    {
                      "type": "string",
                      "description": "%configuration.endpoints.models.string.description%"
                    },
                    {
                      "type": "object",
                      "required": [
                        "id"
                      ],
                      "properties": {
                        "id": {
                          "type": "string",
                          "description": "%configuration.endpoints.models.id.description%"
                        },
                        "name": {
                          "type": "string",
                          "description": "%configuration.endpoints.models.name.description%"
                        },
                        "family": {
                          "type": "string",
                          "description": "%configuration.endpoints.models.family.description%"
                        },
                        "maxInputTokens": {
                          "type": "number",
                          "description": "%configuration.endpoints.models.maxInputTokens.description%",
                          "default": 128000
                        },
                        "maxOutputTokens": {
                          "type": "number",
                          "description": "%configuration.endpoints.models.maxOutputTokens.description%",
                          "default": 8192
                        },
                        "tokenizer": {
                          "type": "string",
                          "description": "%configuration.endpoints.models.tokenizer.description%",
                          "default": "default",
                          "enum": [
                            "default",
                            "conservative",
                            "char4",
                            "openai",
                            "deepseek"
                          ],
                          "enumDescriptions": [
                            "%configuration.endpoints.models.tokenizer.enumDescriptions.0%",
                            "%configuration.endpoints.models.tokenizer.enumDescriptions.1%",
                            "%configuration.endpoints.models.tokenizer.enumDescriptions.2%",
                            "%configuration.endpoints.models.tokenizer.enumDescriptions.3%",
                            "%configuration.endpoints.models.tokenizer.enumDescriptions.4%"
                          ]
                        },
                        "tokenCountMultiplier": {
                          "type": "number",
                          "description": "%configuration.endpoints.models.tokenCountMultiplier.description%",
                          "default": 1,
                          "exclusiveMinimum": 0
                        },
                        "capabilities": {
                          "type": "object",
                          "description": "%configuration.endpoints.models.capabilities.description%",
                          "properties": {
                            "toolCalling": {
                              "anyOf": [
                                {
                                  "type": "boolean",
                                  "description": "%configuration.endpoints.models.capabilities.toolCalling.boolean.description%"
                                },
                                {
                                  "type": "integer",
                                  "description": "%configuration.endpoints.models.capabilities.toolCalling.integer.description%"
                                }
                              ]
                            },
                            "imageInput": {
                              "type": "boolean",
                              "description": "%configuration.endpoints.models.capabilities.imageInput.description%"
                            }
                          }
                        },
                        "parallelToolCalling": {
                          "type": "boolean",
                          "description": "%configuration.endpoints.models.parallelToolCalling.description%"
                        },
                        "stream": {
                          "type": "boolean",
                          "description": "%configuration.endpoints.models.stream.description%"
                        },
                        "temperature": {
                          "type": "number",
                          "description": "%configuration.endpoints.models.temperature.description%"
                        },
                        "topK": {
                          "type": "integer",
                          "description": "%configuration.endpoints.models.topK.description%"
                        },
                        "topP": {
                          "type": "number",
                          "description": "%configuration.endpoints.models.topP.description%"
                        },
                        "presencePenalty": {
                          "type": "number",
                          "description": "%configuration.endpoints.models.presencePenalty.description%"
                        },
                        "frequencyPenalty": {
                          "type": "number",
                          "description": "%configuration.endpoints.models.frequencyPenalty.description%"
                        },
                        "verbosity": {
                          "type": "string",
                          "description": "%configuration.endpoints.models.verbosity.description%",
                          "enum": [
                            "low",
                            "medium",
                            "high"
                          ],
                          "enumDescriptions": [
                            "%configuration.endpoints.models.verbosity.enumDescriptions.0%",
                            "%configuration.endpoints.models.verbosity.enumDescriptions.1%",
                            "%configuration.endpoints.models.verbosity.enumDescriptions.2%"
                          ]
                        },
                        "thinking": {
                          "type": "object",
                          "description": "%configuration.endpoints.models.thinking.description%",
                          "oneOf": [
                            {
                              "properties": {
                                "type": {
                                  "const": "enabled",
                                  "description": "%configuration.endpoints.models.thinking.enabled.type.description%"
                                },
                                "budgetTokens": {
                                  "type": "integer",
                                  "description": "%configuration.endpoints.models.thinking.enabled.budgetTokens.description%"
                                },
                                "effort": {
                                  "type": "string",
                                  "description": "%configuration.endpoints.models.thinking.enabled.effort.description%",
                                  "enum": [
                                    "none",
                                    "minimal",
                                    "low",
                                    "medium",
                                    "high",
                                    "xhigh"
                                  ],
                                  "enumDescriptions": [
                                    "%configuration.endpoints.models.thinking.enabled.effort.enumDescriptions.0%",
                                    "%configuration.endpoints.models.thinking.enabled.effort.enumDescriptions.1%",
                                    "%configuration.endpoints.models.thinking.enabled.effort.enumDescriptions.2%",
                                    "%configuration.endpoints.models.thinking.enabled.effort.enumDescriptions.3%",
                                    "%configuration.endpoints.models.thinking.enabled.effort.enumDescriptions.4%",
                                    "%configuration.endpoints.models.thinking.enabled.effort.enumDescriptions.5%"
                                  ]
                                }
                              },
                              "required": [
                                "type"
                              ]
                            },
                            {
                              "properties": {
                                "type": {
                                  "const": "auto",
                                  "description": "%configuration.endpoints.models.thinking.auto.type.description%"
                                },
                                "budgetTokens": {
                                  "type": "integer",
                                  "description": "%configuration.endpoints.models.thinking.auto.budgetTokens.description%"
                                },
                                "effort": {
                                  "type": "string",
                                  "description": "%configuration.endpoints.models.thinking.auto.effort.description%",
                                  "enum": [
                                    "none",
                                    "minimal",
                                    "low",
                                    "medium",
                                    "high",
                                    "xhigh"
                                  ],
                                  "enumDescriptions": [
                                    "%configuration.endpoints.models.thinking.auto.effort.enumDescriptions.0%",
                                    "%configuration.endpoints.models.thinking.auto.effort.enumDescriptions.1%",
                                    "%configuration.endpoints.models.thinking.auto.effort.enumDescriptions.2%",
                                    "%configuration.endpoints.models.thinking.auto.effort.enumDescriptions.3%",
                                    "%configuration.endpoints.models.thinking.auto.effort.enumDescriptions.4%",
                                    "%configuration.endpoints.models.thinking.auto.effort.enumDescriptions.5%"
                                  ]
                                }
                              },
                              "required": [
                                "type"
                              ]
                            },
                            {
                              "properties": {
                                "type": {
                                  "const": "disabled",
                                  "description": "%configuration.endpoints.models.thinking.disabled.type.description%"
                                }
                              },
                              "required": [
                                "type"
                              ]
                            }
                          ]
                        },
                        "webSearch": {
                          "type": "object",
                          "description": "%configuration.endpoints.models.webSearch.description%",
                          "properties": {
                            "enabled": {
                              "type": "boolean",
                              "description": "%configuration.endpoints.models.webSearch.enabled.description%"
                            },
                            "maxUses": {
                              "type": "integer",
                              "description": "%configuration.endpoints.models.webSearch.maxUses.description%"
                            },
                            "allowedDomains": {
                              "type": "array",
                              "description": "%configuration.endpoints.models.webSearch.allowedDomains.description%",
                              "items": {
                                "type": "string"
                              }
                            },
                            "blockedDomains": {
                              "type": "array",
                              "description": "%configuration.endpoints.models.webSearch.blockedDomains.description%",
                              "items": {
                                "type": "string"
                              }
                            },
                            "userLocation": {
                              "type": "object",
                              "description": "%configuration.endpoints.models.webSearch.userLocation.description%",
                              "properties": {
                                "type": {
                                  "type": "string",
                                  "const": "approximate",
                                  "description": "%configuration.endpoints.models.webSearch.userLocation.type.description%"
                                },
                                "city": {
                                  "type": "string",
                                  "description": "%configuration.endpoints.models.webSearch.userLocation.city.description%"
                                },
                                "region": {
                                  "type": "string",
                                  "description": "%configuration.endpoints.models.webSearch.userLocation.region.description%"
                                },
                                "country": {
                                  "type": "string",
                                  "description": "%configuration.endpoints.models.webSearch.userLocation.country.description%"
                                },
                                "timezone": {
                                  "type": "string",
                                  "description": "%configuration.endpoints.models.webSearch.userLocation.timezone.description%"
                                }
                              },
                              "required": [
                                "type"
                              ]
                            }
                          }
                        },
                        "memoryTool": {
                          "type": "boolean",
                          "description": "%configuration.endpoints.models.memoryTool.description%"
                        },
                        "extraHeaders": {
                          "type": "object",
                          "description": "%configuration.endpoints.models.extraHeaders.description%",
                          "additionalProperties": {
                            "type": "string"
                          }
                        },
                        "extraBody": {
                          "type": "object",
                          "description": "%configuration.endpoints.models.extraBody.description%",
                          "additionalProperties": true
                        }
                      }
                    }
                  ]
                },
                "minItems": 0
              }
            }
          },
          "default": []
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "l10n:check": "bun run scripts/l10n-audit.ts",
    "l10n:sync": "bun run scripts/l10n-audit.ts --write --write-locales",
    "count-loc": "bun run scripts/count-loc.ts",
    "release": "bun run scripts/release.ts"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/vscode": "^1.104.0",
    "@vscode/l10n-dev": "^0.0.35",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "@google/genai": "^1.40.0",
    "@huggingface/tokenizers": "^0.1.1",
    "@iarna/toml": "^2.2.5",
    "@vscode/l10n": "^0.0.18",
    "ollama": "^0.6.3",
    "openai": "^6.18.0",
    "tiktoken": "^1.0.22"
  }
